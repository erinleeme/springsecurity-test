<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flab.daily.mapper.MemberMapper">
    <select id="getMember" parameterType="java.lang.String" resultType="com.example.demo.dao.MemberDAO">
        SELECT
        *
        FROM
        member
        WHERE
        email = #{email}
    </select>

    <update id="addRefreshToken" parameterType="java.util.Map">
        UPDATE member
        SET refresh_token = #{hashRefreshtoken}
        WHERE member_id = #{memberId}
    </update>
</mapper>